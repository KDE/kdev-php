#: duchain/builders/declarationbuilder.cpp:185
#: duchain/builders/declarationbuilder.cpp:200
#: duchain/builders/declarationbuilder.cpp:216
#: duchain/builders/declarationbuilder.cpp:877
#, kde-format
msgid "Cannot use '%1' as class name as it is reserved"
msgstr "Ne eblas uzi '%1' kiel klasnomo ĉar ĝi estas rezervata"

#: duchain/builders/declarationbuilder.cpp:339
#, kde-format
msgid "Access type for interface method %1 must be omitted."
msgstr "Alirtipo por interfaca metodo %1 devas esti ellasita."

#: duchain/builders/declarationbuilder.cpp:343
#, kde-format
msgid "Interface function %1 cannot contain body."
msgstr "Interfaca funkcio %1 ne povas enhavi korpon."

#: duchain/builders/declarationbuilder.cpp:355
#, kde-format
msgid ""
"Class %1 contains abstract method %2 and must therefore be declared abstract "
"or implement the method."
msgstr ""
"Klaso %1 enhavas abstraktan metodon %2 kaj tial devas esti deklarita "
"abstrakta aŭ efektivigi la metodon."

#: duchain/builders/declarationbuilder.cpp:361
#, kde-format
msgid "Abstract function %1 cannot contain body."
msgstr "Abstrakta funkcio %1 ne povas enhavi korpon."

#: duchain/builders/declarationbuilder.cpp:364
#, kde-format
msgid "Cannot use the final modifier on an abstract class member."
msgstr "Ne povas uzi la finan modifilon ĉe abstrakta klasano."

#: duchain/builders/declarationbuilder.cpp:374
#, kde-format
msgid "Non-abstract method %1 must contain body."
msgstr "Ne-abstrakta metodo %1 devas enhavi korpon."

#: duchain/builders/declarationbuilder.cpp:392
#, kde-format
msgid "Cannot use 'final' as constant modifier"
msgstr "Ne povas uzi 'fina' kiel konstantan modifilon"

#: duchain/builders/declarationbuilder.cpp:395
#, kde-format
msgid "Cannot use 'static' as constant modifier"
msgstr "Ne povas uzi 'senmovan' kiel konstantan modifilon"

#: duchain/builders/declarationbuilder.cpp:398
#, kde-format
msgid "Cannot use 'abstract' as constant modifier"
msgstr "Ne povas uzi 'abstrakta' kiel konstanta modifilo"

#: duchain/builders/declarationbuilder.cpp:412
#, kde-format
msgid "Properties cannot be declared final."
msgstr "Proprecoj ne povas esti deklaritaj finaj."

#: duchain/builders/declarationbuilder.cpp:415
#, kde-format
msgid "Properties cannot be declared abstract."
msgstr "Proprecoj ne povas esti deklaritaj abstraktaj."

#: duchain/builders/declarationbuilder.cpp:461
#, kde-format
msgid ""
"Trait method %1 has not been applied, because there are collisions with "
"other trait methods on %2"
msgstr ""
"Trajtometodo %1 ne estis aplikata, ĉar estas kolizioj kun aliaj "
"trajtometodoj sur %2"

#: duchain/builders/declarationbuilder.cpp:654
#, kde-format
msgid "Traits cannot have constants."
msgstr "Trajtoj ne povas havi konstantojn."

#: duchain/builders/declarationbuilder.cpp:661
#, kde-format
msgid ""
"A class constant must not be called 'class'; it is reserved for class name "
"fetching"
msgstr ""
"Klaskonstanto ne devas esti nomita 'klaso'; ĝi estas rezervita por alportado "
"de klasnomoj"

#: duchain/builders/declarationbuilder.cpp:742
#: duchain/builders/declarationbuilder.cpp:768
#, kde-format
msgid "Cannot use 'abstract' as method modifier"
msgstr "Ne povas uzi 'abstraktan' kiel metodomodifilon"

#: duchain/builders/declarationbuilder.cpp:745
#: duchain/builders/declarationbuilder.cpp:771
#, kde-format
msgid "Cannot use 'final' as method modifier"
msgstr "Ne povas uzi 'fina' kiel metodomodifilon"

#: duchain/builders/declarationbuilder.cpp:748
#: duchain/builders/declarationbuilder.cpp:774
#, kde-format
msgid "Cannot use 'static' as method modifier"
msgstr "Ne povas uzi 'senmovan' kiel metodomodifilon"

#: duchain/builders/declarationbuilder.cpp:823
#, kde-format
msgid "Variadic parameter cannot have a default value"
msgstr "Variada parametro ne povas havi defaŭltan valoron"

#: duchain/builders/declarationbuilder.cpp:826
#, kde-format
msgid "Default value for parameters with a class type hint can only be NULL."
msgstr ""
"Defaŭlta valoro por parametroj kun klasspeca sugesto povas esti nur NULL."

#: duchain/builders/declarationbuilder.cpp:843
#, kde-format
msgid "Only the last parameter can be variadic."
msgstr "Nur la lasta parametro povas esti variada."

#: duchain/builders/declarationbuilder.cpp:957
#, kde-format
msgid "Cannot redeclare PHP internal %1."
msgstr "Ne povas redeklari PHP-internan %1."

#: duchain/builders/declarationbuilder.cpp:960
#, kde-format
msgid ""
"%1 and %2 define the same property (%3) in the composition of %1. This might "
"be incompatible, to improve maintainability consider using accessor methods "
"in traits instead."
msgstr ""
"%1 kaj %2 difinas la saman econ (%3) en la kunmetaĵo de %1. Ĉi tio povus "
"esti malkongrua, por plibonigi konserveblecon, konsideru uzi akcesorajn "
"metodojn en trajtoj anstataŭe."

#: duchain/builders/declarationbuilder.cpp:968
#, kde-format
msgid "Cannot redeclare %1, already declared in %2 on line %3."
msgstr "Ne povas redeklari %1, jam deklarita en %2 sur linio %3."

#: duchain/builders/declarationbuilder.cpp:1054
#, kde-format
msgid ""
"Class %1 contains %2 abstract methods and must therefore be declared "
"abstract or implement the remaining methods."
msgstr ""
"Klaso %1 enhavas %2 abstraktajn metodojn kaj tial devas esti deklarita "
"abstrakta aŭ efektivigi la ceterajn metodojn."

#: duchain/builders/declarationbuilder.cpp:1061
#, kde-format
msgid "Cannot re-assign $this."
msgstr "Ne povas reasigni $this."

#: duchain/builders/declarationbuilder.cpp:1255
#, kde-format
msgid "Cannot access private property %1"
msgstr "Ne povas aliri privatan proprecon %1"

#: duchain/builders/declarationbuilder.cpp:1258
#, kde-format
msgid "Cannot access protected property %1"
msgstr "Ne povas aliri protektitan proprecon %1"

#: duchain/builders/declarationbuilder.cpp:1404
#, kde-format
msgid "Cannot use positional argument after argument unpacking"
msgstr "Ne povas uzi pozician argumenton post argumenta malpakado"

#: duchain/builders/declarationbuilder.cpp:1628
#, kde-format
msgid "The use statement with non-compound name '%1' has no effect."
msgstr "La uza deklaro kun nekunmetita nomo '%1' ne efikas."

#: duchain/builders/declarationbuilder.cpp:1655
#, kde-format
msgid "Cannot use '%1' as '%2' because the name is already in use."
msgstr "Ne povas uzi '%1' kiel '%2' ĉar la nomo jam estas uzata."

#: duchain/builders/declarationbuilder.cpp:1681
#, kde-format
msgid "Cannot use %1 as %2 because '%2' is a special class name"
msgstr "Ne povas uzi %1 kiel %2 ĉar '%2' estas speciala klasnomo"

#: duchain/builders/declarationbuilder.cpp:1691
#, kde-format
msgid "The 'yield' expression can only be used inside a function"
msgstr "La 'rendimento' esprimo povas esti uzata nur ene de funkcio"

#: duchain/builders/typebuilder.cpp:412
#, kde-format
msgid "Default value for parameters with an object type can only be NULL."
msgstr "Defaŭlta valoro por parametroj kun objektotipo povas esti nur NULL."

#: duchain/builders/usebuilder.cpp:202
#, kde-format
msgid "Usage of %1 is deprecated."
msgstr "Uzado de %1 estas evitinda."

#: duchain/builders/usebuilder.cpp:204
#, kde-format
msgid "Declaration not found: %1"
msgstr "Deklaro ne trovita: %1"

