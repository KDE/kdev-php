msgstr "%1(e)n erabilera %2(e)tik<br />"

#: duchain/navigation/declarationnavigationcontext.cpp:140
#, kde-format
msgid "Broken member alias trait."
msgstr "Partaide-ezizenaren bereizgarri («trait») hautsita."

